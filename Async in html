    <h1>Async and await in JS</h1>
    <h2 id ="A"></h2>
    <script>
        async function add(a,b) {
            var response = await a+b
            display (response)
        }
        function display(sum){
            document.getElementById("A").innerHTML = sum
        }
            add(234567890,8987654321)
    </script>
