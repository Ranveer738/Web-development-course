class person2:
    def __init__(self,fname,lname):
        self.fname=fname
        self.lname=lname
    def printname(self):
        print("full name is",self.fname)
        print("last name is",self.lname)
class student(person2):
    def __init__(self,fname,lname,year):
        self.year=year
        super().__init__(fname,lname)
obj=student("ranveer","jain",1999)
obj.printname()
print(obj.year)
    
