import turtle
turtle.Screen().bgcolor("blue")
x=turtle.Screen()
x.setup(400,300)
y=turtle.Turtle()
y.forward(100)
y.left(120)
y.forward(100)
y.left(120)
y.forward(100)
y.penup()
y.right(150)
y.forward(50)
y.pendown()
y.right(90)
y.forward(100)
y.right(120)
y.forward(100)
y.right(120)
y.forward(100)
turtle.done()
