        var a = "I am Palak mam student student";
        var b = a.replace(/student/g,"Student2")
        document.write(b +"<br>")
        var c = "RANVEER,ranveer";
                var e = c.replace(/RANVEER/i, "Codingal")
                document.write(e + "<br")
                var f = "I am Modi";
                document.write(f.search(/Modi/))
