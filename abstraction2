from abc import ABC
class animal(ABC):
    def move(self):
        pass
class human(animal):
    def move(self):
        print("I can walk and run")
class snake(animal):
    def move(self):
        print("I can bite and crawl")
class dog(animal):
    def move(self):
        print("I can bark and run")
class lion(animal):
    def move(self):
        print("I can eat and scare")
obj=human()
obj.move()
x=snake()
x.move()
z=dog()
z.move()
a=lion()
a.move()

