def introduce_yourself(name):
    print("Hello,good morning I am",name)
introduce_yourself("Ranveer")
introduce_yourself("Sohan")
def introduce_input(age):
    print("My age is",age)
age=int(input("Enter your age:"))
introduce_input(age)
def inm(a,b):
    c=a+b
    return c
a=int(input("Enter a no:"))
b=int(input("Enter a no:"))
print("Addition is",inm(a,b))

    
