    <h2>Sum of two Numbers</h2>
    <h3 id="result"></h3>
    <script>
        var num1 = 5; // First number
        var num2 = 10; // Second number
        var sum = num1 + num2; // Calculate the sum of num1 and num2
        document.getElementById('result').innerHTML = "The sum of " + num1 + "and" + num2 + "=" + sum        // Display the result
        // End of script
    </script>
    
