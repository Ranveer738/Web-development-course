    <meta name="viewport" content="width=<device-width>, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Call stack</h1>
    <h2 id="callstack"></h2>
    <script>
        function add(a, b) {
            return a + b;
        }
        function average(a, b) {
            return add(a, b) / 2;
        }
        let result = average(10, 20);
        document.getElementById("callstack").innerHTML = "The average of 10 and 20 is: " + result;
    </script>
</body>
</html>
