    <button onclick="myFunction()">Execute JS Function</button>
    <button onclick="myFunction1(5,10)">Execute</button>
    <script>
        function myFunction() {
           document.write("Hello, this is a JavaScript function!");
        }
        function myFunction1(x,y) {
            document.write(x+y)
        } 
        </script>

    
