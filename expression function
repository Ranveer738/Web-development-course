    <h1 style="color: green;">Expression Function</h1>
    <script>
        var result = function(a,b) {
            return a * b;
        };
        document.write("The result of the expression function is: " + result(35, 10.9) + "<br>");
        var divi = function(a, b) {
            return a/b;
        };
        document.write("The division of 1.2 and 2.3 is: " + divi(1.2, 2.3) + "<br>");
        </script>

    
