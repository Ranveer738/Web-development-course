<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>JS Classes</h1>
    <h2 id ="a"></h2>
    <h2 id="b"></h2>
    <script>
        class Student {
            constructor(name, year) {
                this.name = name;
                this.year = year;
            }

            age() {
                let date = new Date();
                return date.getFullYear() - this.year;
            }
        }
        Student1 = new Student("John", 2011);
        document.getElementById("a").innerHTML = "Name: " + Student1.name + ", Age: " + Student1.age();
        Student2 = new Student("Jane", 2015);
        document.getElementById("b").innerHTML = "Name: " + Student2.name + ", Age: " + Student2.age();     
    </script>
</body>
</html>
