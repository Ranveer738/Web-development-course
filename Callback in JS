    <h1>Callback in JS</h1>
    <script>
        function abcd (value){
            document.write(value)
        }
        function xyz (n1,n2,n3) {
            var sum = n1+n2
            n3(sum)
        }
        xyz(976546785,1234567890,abcd)
    </script>
