    <h1>Javascript Methods</h1>
    <script>
        var x = "Hello, World!";
        document.write(x.toUpperCase()+"<br>");
        document.write(x.toLowerCase()+"<br>");
        document.write(x.startsWith("H")+"<br>")
        var y = "I Learnt about Methods";
        var z = new RegExp("about");
        var result = z.test(y);
        document.write(result+"<br>");
        document.write(x.charAt(0)+"<br>");
        document.write(x.indexOf("World")+"<br>");
    </script>
    
